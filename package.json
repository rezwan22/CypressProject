{
  "name": "cypress-tlgo",
  "version": "1.0.0",
  "description": "TLGO Automation Test Suite",
  "main": "index.js",
  "scripts": {
    "pretest": "rm -r ./cypress/report/*.json",
    "test": "npm run cypress-test || npm run posttest",
    "cypress-test": "cypress run",
    "posttest": "npm run merge-reports && npm run generate-html-report",
    "merge-reports": "mochawesome-merge ./cypress/report/*.json > ./report1.json",
    "generate-html-report": "marge report1.json --reportDir cypress/report"

  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cypress": "^9.5.3",
    "cypress-downloadfile": "^1.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "cypress-file-upload": "^5.0.8"
  }
}
